{
  "name": "swing-analyzer",
  "version": "1.0.0",
  "type": "module",
  "description": "Web-based swing analyzer using MediaPipe",
  "scripts": {
    "dev": "vite",
    "prebuild": "./scripts/generate-version.sh",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug"
  },
  "keywords": [
    "pose",
    "detection",
    "swing",
    "analysis",
    "mediapipe"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mediapipe/pose": "^0.5.1675469404",
    "@tensorflow-models/pose-detection": "^2.1.0",
